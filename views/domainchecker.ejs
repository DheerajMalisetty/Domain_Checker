    <div class="container">
        <h1>Enter your word:</h1>
        <form action="/check" method="post">
            <input class="form-control" id="input" type="text" name="input"><br><br>

            <input class="btn btn-primary" type="submit" name="submit" value="Check">
        </form>
    </div>
    <div class="container">
        <h1>Domain Status:</h1><br>


            <div class="row">

                   <% name.forEach(function(names) { %>
                    <% if (names[1]){ %>

                        <div class="col-sm-2 valid"><%= names[0] %></div>
                    <%}%>
                    <% if (names[1]==false) {%>
                        <div class="col-sm-2 invalid"><%= names[0] %></div>
                    <% } %>

                    <% }); %>

            </div>
    </div>
</div>


